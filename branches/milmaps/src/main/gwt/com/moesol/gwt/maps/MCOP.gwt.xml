<module rename-to='mcop'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />
	<inherits name="com.google.gwt.xml.XML" />
	<inherits name="mil.GeotransJ" />
	<inherits name="com.google.gwt.json.JSON"/>

	<set-property name='user.agent' value='safari' />
	<stylesheet src='test-stylesheet.css' />
	<entry-point class='com.moesol.gwt.maps.client.MCOP' />
	<inherits name="com.google.gwt.inject.Inject"/>
	
	<source path="client" excludes="**/*Test.java,**/Test*.java"/>
	<source path="shared" excludes="**/*Test.java,**/Test*.java"/>
</module>
